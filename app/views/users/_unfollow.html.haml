- Rails.logger.info "post/unfollow rendered"
= form_for(current_user.active_relationships.build, :url => url_for( :controller => 'relationships', :action => :create) , remote: true, :html => { :class => "unfollow_form_#{@user.id}"}) do |f|
  %div= hidden_field_tag :unfollowed_id, @user.id
  = f.submit "unfollow", :class => 'relationship'
